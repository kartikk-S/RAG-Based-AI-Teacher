import os
import subprocess

files=os.listdir("videos")

for file in files:
    print(file)
    tutorial_no=file.split(".mp4")[0].split('#')[1]
    file_name=file.split("_")[0]
    print(tutorial_no, file_name)
    subprocess.run(["ffmpeg", "-i", f"videos/{file}", f"audios/{tutorial_no}_{file_name}.mp3"])